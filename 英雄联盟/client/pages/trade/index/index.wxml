<view style="margin:10rpx 0rpx 20rpx 20rpx; border-bottom:1rpx solid #ddd "
	  wx:for="{{teams}}" 
      wx:key="teamsItem">

	<navigator url="../detail/detail?objectId={{item.objectId}}&title={{item.title}}">
